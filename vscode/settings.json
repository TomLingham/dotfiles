{
  "[groovy]": {
    "editor.tabSize": 4
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[makefile]": {
    "editor.insertSpaces": false
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.rulers": [
    80
  ],
  "cSpell.language": "en-GB",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.codeLens": false,
  "editor.fontFamily": "FiraCode Nerd Font",
  "editor.fontLigatures": true,
  "editor.fontSize": 13,
  "editor.renderWhitespace": "all",
  "editor.inlayHints.enabled": "offUnlessPressed",
  "editor.tabSize": 2,
  "eslint.validate": ["typescript", "typescriptreact"],
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "files.associations": {
    "Dockerfile*": "dockerfile",
    "Jenkinsfile*": "groovy"
  },
  "http.proxyStrictSSL": false,
  "javascript.validate.enable": false,
  "prettier.requireConfig": true,
  "search.useIgnoreFiles": true,
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.scrollback": 6000,
  "terminal.integrated.shellIntegration.enabled": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vim.enableNeovim": true,
  "vim.leader": "<space>",
  "vim.neovimPath": "/opt/homebrew/bin/nvim",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "f"],
      "commands": [
        {
          "command": "editor.action.formatDocument"
        }
      ]
    },
    {
      "before": ["<leader>", "g"],
      "commands": [
        {
          "command": "editor.action.revealDefinition"
        }
      ]
    },
    {
      "before": ["<leader>", "r"],
      "commands": [
        {
          "command": "editor.action.rename"
        }
      ]
    },
    {
      "before": ["<leader>", "d"],
      "commands": [
        {
          "command": "docthis.documentThis"
        }
      ]
    }
  ],
  "vim.useCtrlKeys": true,
  "vsicons.dontShowNewVersionMessage": true,
  "workbench.colorTheme": "Catppuccin Mocha",
  "workbench.iconTheme": "vscode-icons",
  "workbench.startupEditor": "newUntitledFile",
  "editor.accessibilitySupport": "off",
  "redhat.telemetry.enabled": false,
  "editor.linkedEditing": true,
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.bracketPairColorization.enabled": true,
  "cSpell.userWords": [
    "fastify",
    "Kubernetes",
    "Myob",
    "openapi",
    "Pepperstone",
    "plugins",
    "reqwest",
    "rolldown",
    "tseslint"
  ],
  "security.workspace.trust.banner": "never",
  "security.workspace.trust.enabled": false,
  "security.workspace.trust.startupPrompt": "never",
  "security.workspace.trust.untrustedFiles": "open",
  "extensions.ignoreRecommendations": true,
  "[dockercompose]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "plantuml.render": "PlantUMLServer",
  "plantuml.server": "http://localhost:7911",
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[rust]": {
    "editor.tabSize": 4,
    "editor.rulers": [100]
  },
  "cSpell.enableFiletypes": [
    "mdx"
  ],
  "[python]": {
    "editor.formatOnType": true
  },
  "editor.inlayHints.fontFamily": "FiraCode Nerd Font",
  "editor.minimap.enabled": false,
  "hexeditor.columnWidth": 32,
  "hexeditor.showDecodedText": true,
  "hexeditor.defaultEndianness": "little",
  "hexeditor.inspectorType": "aside",
  "window.commandCenter": false,
  "workbench.layoutControl.enabled": false,
  "application.shellEnvironmentResolutionTimeout": 20,
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll": "never",
    "source.fixAll.eslint": "never"
  },
  "markdown.extension.toc.updateOnSave": false,
  "workbench.tree.enableStickyScroll": false,
  "workbench.activityBar.location": "hidden",
  "github.copilot.editor.enableAutoCompletions": true,
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  }
}
